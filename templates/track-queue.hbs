<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Length</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    {{#each orderedTracks}}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.length}}s</td>
      <td>{{this.download_status}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

